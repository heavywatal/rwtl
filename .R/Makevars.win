CXX14 = $(BINPREF)g++ $(M_ARCH)
CXX17 = $(BINPREF)g++ $(M_ARCH)
CXX14FLAGS = -O2 -Wall $(DEBUGFLAG) -mtune=native
CXX17FLAGS = -O2 -Wall $(DEBUGFLAG) -mtune=native
CXX14STD = -std=c++14
CXX17STD = -std=c++1z
CPPFLAGS = -IC:/cygwin64/home/$(USERNAME)/local/include -IC:/cygwin64/usr/include
LDFLAGS = -LC:/cygwin64/home/$(USERNAME)/local/lib -LC:/cygwin64/usr/lib
